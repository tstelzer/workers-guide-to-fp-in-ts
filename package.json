{
    "name": "workers-guide-to-static-functional-programming",
    "version": "0.1.0",
    "description": "An introduction to static functional programming in typescript.",
    "main": "index.ts",
    "scripts": {
        "clean": "del docs",
        "build:html": "ts-node --pretty source/index.ts -D",
        "build:styles": "sass --no-source-map --style=compressed source/styles/index.scss docs/styles.css",
        "build": "npm-run-all clean build:*",
        "serve": "http-server docs -p 3000",
        "dev:styles": "npm run build:styles && npm run build:styles -- --watch",
        "dev:html": "nodemon",
        "start": "npm run clean && npm-run-all -p serve dev:*"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tstelzer/workers-guide-to-fp-in-ts"
    },
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@mapbox/rehype-prism": "^0.5.0",
        "fp-ts": "^2.8.1",
        "glob": "^7.1.6",
        "io-ts": "^2.2.9",
        "io-ts-types": "^0.5.9",
        "monocle-ts": "^2.3.3",
        "nodemon": "^2.0.4",
        "prismjs": "^1.21.0",
        "ramda": "^0.27.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-markdown": "^4.3.1",
        "refractor": "^3.0.0",
        "rehype-parse": "^7.0.1",
        "rehype-react": "^6.1.0",
        "rehype-slug": "^3.0.0",
        "rehype-toc": "^3.0.2",
        "remark": "^12.0.1",
        "remark-code-import": "^0.2.0",
        "remark-frontmatter": "^2.0.0",
        "remark-parse": "^8.0.3",
        "remark-parse-yaml": "0.0.3",
        "remark-rehype": "^7.0.0",
        "ruins-ts": "0.0.2",
        "rxjs": "^6.6.2",
        "sass": "^1.26.10",
        "to-vfile": "^6.1.0",
        "ts-node": "^8.10.2",
        "unified": "^9.1.0",
        "unist-util-visit": "^2.0.3",
        "vfile": "^4.2.0"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/node": "^14.0.27",
        "@types/ramda": "^0.27.14",
        "@types/react": "^16.9.46",
