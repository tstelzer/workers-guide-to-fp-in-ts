{
    "name": "workers-guide-to-static-functional-programming",
    "version": "0.1.0",
    "description": "An introduction to static functional programming in typescript.",
    "main": "index.ts",
    "scripts": {
        "clean": "trash doc",
        "build:html": "ts-node --pretty source/index.ts -D",
        "build:styles": "node-sass --omit-source-map-url --output-style=compressed source/styles/index.scss doc/styles.css",
        "build": "npm-run-all clean build:*",
        "serve": "http-server doc -p 3000",
        "dev:styles": "npm run build:styles && npm run build:styles -- --watch",
        "dev:html": "nodemon",
        "start": "npm run clean && npm-run-all -p serve dev:*"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/tstelzer/workers-guide-to-fp-in-ts"
    },
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@mapbox/rehype-prism": "^0.4.0",
        "fp-ts": "^2.4.3",
        "glob": "^7.1.6",
        "io-ts": "^2.0.6",
        "io-ts-types": "^0.5.5",
        "monocle-ts": "^2.1.0",
        "node-sass": "^4.13.1",
        "nodemon": "^2.0.2",
        "prismjs": "^1.19.0",
        "ramda": "^0.26.1",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-markdown": "^4.3.1",
        "refractor": "^2.10.1",
        "rehype-parse": "^6.0.2",
        "rehype-react": "^4.0.1",
        "rehype-slug": "^2.0.3",
        "rehype-toc": "^2.2.3",
        "remark": "^11.0.2",
        "remark-frontmatter": "^1.3.2",
        "remark-parse": "^7.0.2",
        "remark-parse-yaml": "0.0.3",
        "remark-rehype": "^5.0.0",
        "ruins-ts": "0.0.2",
        "rxjs": "^6.5.4",
        "to-vfile": "^6.0.0",
        "ts-node": "^8.6.2",
        "unified": "^8.4.2",
        "unist-util-visit": "^2.0.1",
        "vfile": "^4.0.2"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/node": "^13.7.0",
        "@types/ramda": "^0.26.40",
        "@types/react": "^16.9.19",
        "@types/react-dom": "^16.9.5",
        "@types/refractor": "^2.8.0",
        "@types/rehype-react": "^4.0.0",
        "@typescript-eslint/eslint-plugin": "^2.18.0",
        "@typescript-eslint/parser": "^2.18.0",
        "@typescript-eslint/typescript-estree": "^2.18.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-react": "^7.18.0",
        "http-server": "^0.12.1",
        "npm-check-updates": "^4.0.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.19.1",
        "trash-cli": "^3.0.0",
        "typescript": "^3.7.5"
    }
}
