        "eslint": "^7.6.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.20.5",
        "http-server": "^0.12.3",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.0.5",
        "del-cli": "^3.0.1",
        "typescript": "^3.9.7"
    }
